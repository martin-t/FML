// Collatz Conjecture

function collatz(n) ->
begin
    let i = n;
    let count = 0;
    while i != 1 do begin
        if i % 2 == 0 then
            i <- i / 2
        else
            i <- 3 * i + 1;
        count <- count + 1;
    end;
    count
end;

let n = 1;
let max = 500;
while n <= max do begin
    let count = collatz(n);
    print("n=~, count=~\n", n, count);
    n <- n + 1;
end;

