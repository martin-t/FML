function println() -> print("\n");

function id(x) -> x;
function id2(a1, a2) -> a2;
function id3(a1, a2, a3) -> a3;
function id4(a1, a2, a3, a4) -> a4;
function id5(a1, a2, a3, a4, a5) -> a5;
function id6(a1, a2, a3, a4, a5, a6) -> a6;
print("~\n", id(42));
print("~\n", id2(42, 43));
print("~\n", id3(42, 43, 44));
print("~\n", id4(42, 43, 44, 45));
print("~\n", id5(42, 43, 44, 45, 46));
print("~\n", id6(42, 43, 44, 45, 46, 47));

function arg1(a1, a2, a3, a4, a5, a6) -> a1;
function arg2(a1, a2, a3, a4, a5, a6) -> a2;
function arg3(a1, a2, a3, a4, a5, a6) -> a3;
function arg4(a1, a2, a3, a4, a5, a6) -> a4;
function arg5(a1, a2, a3, a4, a5, a6) -> a5;
function arg6(a1, a2, a3, a4, a5, a6) -> a6;
print("~\n", arg1(42, 43, 44, 45, 46, 47));
print("~\n", arg2(42, 43, 44, 45, 46, 47));
print("~\n", arg3(42, 43, 44, 45, 46, 47));
print("~\n", arg4(42, 43, 44, 45, 46, 47));
print("~\n", arg5(42, 43, 44, 45, 46, 47));
print("~\n", arg6(42, 43, 44, 45, 46, 47));

function four() -> 4;
print("~\n", four());

println();

function sub2(a1, a2) -> a1 - a2;
function sub3(a1, a2, a3) -> a1 - a2 - a3;
function sub4(a1, a2, a3, a4) -> a1 - a2 - a3 - a4;
function sub5(a1, a2, a3, a4, a5) -> a1 - a2 - a3 - a4 - a5;
function sub6(a1, a2, a3, a4, a5, a6) -> a1 - a2 - a3 - a4 - a5 - a6;
print("~\n", sub2(42, 43));
print("~\n", sub3(42, 43, 44));
print("~\n", sub4(42, 43, 44, 45));
print("~\n", sub5(42, 43, 44, 45, 46));
print("~\n", sub6(42, 43, 44, 45, 46, 47));

function div2(a1, a2) -> a1 / a2;
function div3(a1, a2, a3) -> a1 / a2 / a3;
function div4(a1, a2, a3, a4) -> a1 / a2 / a3 / a4;
function div5(a1, a2, a3, a4, a5) -> a1 / a2 / a3 / a4 / a5;
function div6(a1, a2, a3, a4, a5, a6) -> a1 / a2 / a3 / a4 / a5 / a6;
print("~\n", div2(5040, 7));
print("~\n", div3(5040, 7, 6));
print("~\n", div4(5040, 7, 6, 5));
print("~\n", div5(5040, 7, 6, 5, 4));
print("~\n", div6(5040, 7, 6, 5, 4, 3));

println();

function rem2(a1, a2) -> a1 % a2;
function rem3(a1, a2, a3) -> a1 % a2 % a3;
print("~\n", rem2(26, 10));
//print("~\n", rem2(-26, 10));
//print("~\n", rem2(26, -10));
//print("~\n", rem2(-26, -10));
print("~\n", rem3(26, 10, 5));

function sum_squares1(x) -> x * x;
function sum_squares2(x, y) -> x * x + y * y;
function sum_squares3(x, y, z) -> x * x + y * y + z * z;
function sum_squares4(x, y, z, w) -> x * x + y * y + z * z + w * w;
print("~\n", sum_squares1(5));
print("~\n", sum_squares2(5, 6));
print("~\n", sum_squares3(5, 6, 7));
print("~\n", sum_squares4(5, 6, 7, 8));

println();

function take_9_args(a1, a2, a3, a4, a5, a6, a7, a8, a9) ->
    a1 * 11 + a2 * 12 + a3 * 13 + a4 * 14 + a5 * 15 + a6 * 16 + a7 * 17 + a8 * 18 + a9 * 19;
print("~\n", take_9_args(1, 2, 3, 4, 5, 6, 7, 8, 9));
print("~\n", take_9_args(9, 8, 7, 6, 5, 4, 3, 2, 1));

